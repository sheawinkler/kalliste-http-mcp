OLLAMA_BASE_URL=http://ollama:11434
# === Qdrant host vs. container URL ===
QDRANT_URL_HOST=http://127.0.0.1:6333
# Match your embedding model dimension. (Ollama's nomic-embed-text is typically 768.)
EMBEDDING_DIM=768
HNSW_M=32
HNSW_EF_CONSTRUCT=128
# Optional: switch to full scan below this count; set 0 to disable
HNSW_FULL_SCAN_THRESHOLD=0
# Store HNSW graph data on disk (useful for very large indexes)
HNSW_ON_DISK=false
# Store raw vectors on disk (memmap) to cut RAM (small latency tradeoff)
VECTORS_ON_DISK=true
# one of: none | scalar | binary | product
QDRANT_QUANTIZATION=scalar
# Scalar quantization (recommended default)
QDRANT_SCALAR_TYPE=int8
QDRANT_SCALAR_QUANTILE=0.99
QDRANT_SCALAR_ALWAYS_RAM=true
# Binary quantization (optional; fast & tiny, but model-dependent)
# encoding: default | two_bits | one_and_half_bits
QDRANT_BINARY_ENCODING=default
QDRANT_BINARY_ALWAYS_RAM=true
# Product quantization (highest compression; slower & lower accuracy)
# compression: x4 | x8 | x16 | x32 (see docs); x16 is a good start for 768d+
QDRANT_PQ_COMPRESSION=x16
QDRANT_PQ_ALWAYS_RAM=true
# --- Qdrant runtime knobs (query-time) ---
QDRANT_RUNTIME_EF_DEFAULT=128      # ef at search time; higher = better recall, more CPU
QDRANT_SEARCH_EXACT_DEFAULT=false  # set true to force exact scan (slow)
# --- Qdrant HNSW build-time (index) ---
QDRANT_HNSW_M=16
QDRANT_HNSW_EF_CONSTRUCT=256
# --- Quantization (choose one: scalar | binary | product | none) ---
QDRANT_QUANTILE=0.99           # scalar-only
QDRANT_ALWAYS_RAM=true         # keep quantized vectors cached in RAM
# --- Embeddings (must match collection vector size) ---
EMBEDDING_MODEL_QDRANT=sentence-transformers/all-MiniLM-L6-v2
QDRANT_DIM=384
QDRANT_DISTANCE=Cosine
# --- Sidecar port (exposed) ---
QDRANT_ADV_PORT=8022
